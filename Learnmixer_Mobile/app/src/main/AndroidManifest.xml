<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eeristi.learnmixer_mobile" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".screen2"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.eeristi.learnmixer_mobile.SCREEN_TWO" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>



        <activity
            android:name=".screen2_1"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.eeristi.learnmixer_mobile.SCREEN_TWO_one" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".screen2_2"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.eeristi.learnmixer_mobile.SCREEN_TWO_two" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".screen2_3"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.eeristi.learnmixer_mobile.SCREEN_TWO_three" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

    </application>

</manifest>
